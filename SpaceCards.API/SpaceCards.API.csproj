	<ItemGroup>
		<ProjectReference Include="..\SpaceCards.BusinessLogic\SpaceCards.BusinessLogic.csproj" />
		<ProjectReference Include="..\SpaceCards.DataAccess.Postgre\SpaceCards.DataAccess.Postgre.csproj" />
